    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "chart.js": "^3.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.4",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typeface-roboto": "^1.1.13",
    "wasm-koala-blog": "file:./wasm-koala-blog/pkg",
    "web-vitals": "^1.0.1",
    "webassembly": "file:./wasm-koala-blog/pkg"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.2",
    "react-app-rewired": "^2.1.8",
    "wasm-loader": "^1.3.0"
  },
  "scripts": {
    "startx": "react-app-rewired start",
    "start": "react-scripts start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "prettier": "prettier --single-quote --no-semi --jsx-bracket-same-line --trailing-comma none --write \"src/**/*.{js,jsx}\"",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "parser": "babylon",
      "style": "module"
    }
  }
}
